<%= javascript_include_tag :defaults %>

<table>
<% form_tag :action => 'search' do %>

  <tr>
    <td><label>Name:</label></td>
    <td><%= text_field_with_auto_complete :computer, :name, {}, {:method => :get} %></td>
    <td><%= submit_tag "Search" %></td>


<% end %>

<% form_tag :action => 'search' do %>


    <td><label>Ip:</label></td>
    <td><%= text_field_with_auto_complete :computer, :ip, {}, {:method => :get} %></td>
    <td><%= submit_tag "Search" %></td>


<% end %>

<% form_tag :action => 'search' do %>


    <td><label>Mac:</label></td>
    <td><%= text_field_with_auto_complete :computer, :mac, {}, {:method => :get} %></td>
    <td><%= submit_tag "Search" %></td>
  </tr>

<% end %>
</table>
