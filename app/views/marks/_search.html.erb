<%= javascript_include_tag :defaults %>

<% content_tag :table do %>
  <% content_tag :tr do %>
    <% form_tag :action => 'search' do %>
    <% content_tag :td do %>
      <%= t('marks.mark') %>
    <% end %>
    <% content_tag :td do %>
       <%= text_field_with_auto_complete :mark, :name,
           {:size => 20, :focus => true}, {:method => :get} %>
    <% end %>
    <% content_tag :td do %>
      <%= content_tag('td', submit_tag(t('search.label'))) %>
    <% end %>
  <% end %>
  <% end %>
<% end %>
